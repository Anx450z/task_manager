<p style="color: green"><%= notice %></p>

<h1>Containers</h1>

<div id="containers" class="container-wrapper">
  <% @containers.each do |container| %>
    <%= render container %>
  <% end %>
</div>

<div class="floating-buttons">
  <%= link_to 'New Container', new_container_path, class: 'floating-button' %>
</div>
